{
  "expo": {
    "name": "Opmundi",
    "slug": "Opmundi",
    "version": "1.1.0",
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "O aplicativo poder√° solicitar alguma foto da sua galeria."
        }
      ],
      [
        "expo-ads-admob",
        {
          "userTrackingPermission": "This identifier will be used to deliver personalized ads to you."
        }
      ]
    ],
    "assetBundlePatterns": ["**/*"],
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0e4121"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#0e4121"
      },
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-8508944925241498~9378061607"
      }
    },
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#0e4121"
    }
  }
}
