{
  "expo": {
    "name": "Opmundi",
    "slug": "Opmundi",
    "version": "1.3.0",
    "assetBundlePatterns": ["**/*"],
    "orientation": "portrait",
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "O aplicativo poder√° solicitar alguma foto da sua galeria."
        }
      ]
    ],
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0e4121"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#0e4121"
      }
    },
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#0e4121"
    }
  }
}
